  skos:definition "Data Structure Definition (DSD) developed for the specific needs of one organisation only."@en ;
  skos:inScheme :glossary ;
  skos:prefLabel "Local DSD"@en ;
  skos:related :DSD_GLOBAL ;
  skos:scopeNote "An example is a structure for use in internal production processes."@en ;
  skos:topConceptOf :glossary ;
.
