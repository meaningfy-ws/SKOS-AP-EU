  skos:related :COVERAGE_GEO ;
  skos:related :COVERAGE_POP ;
  skos:related :COVERAGE_TIME ;
  skos:related :SECTOR ;
  skos:scopeNote "This metadata element lists groups and subgroups of related activities or populations covered by the data set produced. These sectors can be institutional sectors, economic or other sectors (e.g. local government, agriculture, forestry, or business services)."@en ;
  skos:topConceptOf :cross_domain ;
  skos:topConceptOf :glossary ;
.
:COVERAGE_TIME
  a skos:Concept ;
  dct:source "SDMX, \"SDMX Glossary Version 1.0\", February 2016" ;
  dct:source "https://sdmx.org/wp-content/uploads/SDMX_Glossary_Version_1_0_February_2016.docx" ;
  skos:definition "Period of time for which data are provided."@en ;
  skos:inScheme :cross_domain ;
  skos:inScheme :glossary ;
  skos:notation "CL_COVERAGE_TIME" ;
  skos:notation "Codelist; String" ;
  skos:prefLabel "Time coverage"@en ;
  skos:related :COVERAGE ;
  skos:related :COVERAGE_GEO ;
  skos:related :COVERAGE_POP ;
  skos:related :COVERAGE_SECTOR ;
  skos:related :COVERAGE_TIME ;
  skos:scopeNote "This metadata element describes the time period(s) covered by the data set. The time period covered can be indicated as a time interval, e.g. \"1985 to 2006\" for annual time series data, or as several intervals or values of time."@en ;
  skos:topConceptOf :cross_domain ;
  skos:topConceptOf :glossary ;
.
